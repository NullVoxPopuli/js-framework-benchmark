{
  "name": "glimmer-app",
  "version": "0.0.0",
  "description": "A brand new Glimmer app.",
  "js-framework-benchmark": {
    "frameworkVersionFromPackage": "@glimmer/application",
    "customURL": "/dist"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build -prod",
    "start": "ember server",
    "test": "ember test",
    "build-dev": "ember build",
    "build-prod": "ember build --environment=production",
    "lint:ts": "tslint -c tslint.json 'src/**/*.ts' -t codeFrame"
  },
  "devDependencies": {
    "@glimmer/application": "^0.13.0",
    "@glimmer/application-pipeline": "^0.13.0",
    "@glimmer/blueprint": "~0.13.0",
    "@glimmer/component": "^0.13.0",
    "@glimmer/inline-precompile": "^1.0.0",
    "@glimmer/resolver": "^0.4.1",
    "@glimmer/test-helpers": "^0.31.1",
    "@types/qunit": "^2.0.31",
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli": "^2.16.2",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-tslint": "^0.1.3",
    "ember-cli-uglify": "^2.0.0",
    "qunitjs": "^2.4.1",
    "typescript": "~2.6.0"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "private": true,
  "dependencies": {}
}
